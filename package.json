{
  "name": "react-parcoords",
  "version": "0.3.0",
  "description": "A react wrapper around d3 parallel-coordinates",
  "author": {
    "name": "Daniel Kostro",
    "email": "kostro.d@gmail.com"
  },
  "files": [
    "lib",
    "lib-es6",
    "d3.parcoords.js",
    "d3.parcoords.css"
  ],
  "main": "lib/index.js",
  "keywords": [
    "react",
    "parcoords",
    "parallel",
    "coordinates"
  ],
  "module": "lib-es6/index.js",
  "scripts": {
    "compile": "babel src --out-dir lib --ignore __tests__ --quiet",
    "compile-es6": "BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore __tests__ --quiet",
    "start": "webpack-dev-server --inline",
    "test": "run-s eslint",
    "eslint": "eslint src webpack.config.js",
    "eslint-fix": "eslint --fix src",
    "prepare": "run-p compile compile-es6",
    "testonly": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.15.0",
    "eslint-config-cheminfo": "^1.12.0",
    "eslint-config-neptune-react": "^2.3.1",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.5",
    "npm-run-all": "^4.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "repository": "neptunejs/react-parcoords",
  "license": "MIT",
  "dependencies": {
    "d3": "^3.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0"
  }
}
